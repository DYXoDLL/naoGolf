                                                                        <content>
                                                                            <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.motionProxy = ALProxy("ALMotion")
        self.CameraProxy = ALProxy("ALVideoDevice")
        self.g_count=0
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #self.log("1")
        import time
        import cv2
        import numpy as np
        #subscriberID = "subscriberID"
        #subscriberID = self.CameraProxy.subscribe(subscriberID, 3, 13, 5);#2:kvga
        #self.CameraProxy.getImageLocal(subscriberID);
        img = cv2.imread('/home/nao/recordings/cameras/image1.jpg')
        #img=imgIn.getdata()
        hsv=cv2.cvtColor(img,cv2.COLOR_BGR2HSV)
        lower_yellow=np.array([20,60,60])
        upper_yellow=np.array([30,240,230])
        mask=cv2.inRange(hsv,lower_yellow,upper_yellow)
        res=cv2.bitwise_and(img,img,mask=mask)
        median=cv2.medianBlur(res,7)
        gray = cv2.cvtColor(median,cv2.COLOR_BGR2GRAY)
        edges = cv2.Canny(gray,200,300)
        lines = None
        lines = cv2.HoughLines(edges,1,np.pi/180,50)
        #self.log("2")
        #print lines
        if lines != None:
            lines1=np.array(filter(lambda x:x[1]<1,lines[0]))
            lines2=np.array(filter(lambda x:x[0]>20,lines1))
            lines3=np.array(filter(lambda x:x[0]<1240,lines2))
            if len(lines3) != 0:
                a=lines3[0][0]
                self.log(str(a))
                lines3.sort(axis=0)
                out = np.median(lines3,axis=0)
                rho = out[0]
                theta = out[1]
                b = np.sin(theta)
                a = np.cos(theta)
                ratio = np.tan(theta)
                x0 = rho/a
                self.log("rho:"+str(rho)+"theta:"+str(theta)+"cos:"+str(a))
                xsum = 0
                ysum = 0
                count = 0
                for i in range(200,720):
                    x=int(x0-ratio*i)
                    if gray[i,x] > 0:
                        xsum+=x
                        ysum+=i
                        count+=1

                if count > 0:
                    xsum/=count
                    ysum/=count
                    self.log("pixel:"+str(xsum))
                    final = (1-xsum/640.0)*0.532
                    self.log("output:"+str(final))
                    self.onStopped(final) #activate the output of the box

                else:
                    self.log("BUG detected")
                    self.NoMarker()
            else:
                self.log("median BUG detected")
                self.NoMarker()
                self.g_count += 1
                if self.g_count ==2:
                    self.g_count = 5
                    self.HeadMove()
                if self.g_count ==30:
                    self.log("find bar error")
                    self.MoveBody()
        else:
            self.log("No object detected")
            self.g_count += 1
            if self.g_count ==2:
                self.g_count = 5
                self.HeadMove()
            if self.g_count ==30:
                self.log("find bar error")
                self.MoveBody()
            self.log(str(self.g_count))
            self.NoMarker()
            pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
</content>
