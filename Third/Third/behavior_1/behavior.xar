        self.bMustStop = True
        pass

    def onInput_onStart(self):

        self.counter2 = 0
        self.bMustStop = False
        while not self.bMustStop:
            self.counter += 1
            self.counter2 += 1

            if self.counter == 1:
                self.output1()
                time.sleep(10.0)
            if self.counter == 2:
                self.output2()
                time.sleep(1.5)
            if self.counter == 3:
                self.output3()
                time.sleep(10.0)
            if self.counter == 4:
                time.sleep(1.5)
                self.output4()
                self.counter = 0
            time.sleep(1)
            if self.counter2 == 2:
                self.counter2 = 0
                self.Problem()
