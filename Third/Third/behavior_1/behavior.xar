        while( not self.mutex.testandset() ):
            time.sleep( 0.01 );
        if( self.nNbrTimesOutputted < self.nNbrMaxOutputting  ):
            self.nNbrTimesOutputted += 1;
            self.output_SendValue( p );
        self.mutex.unlock();

    def onInput_onReset(self):
